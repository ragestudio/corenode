{
  "name": "@nodecorejs/debugger-ui",
  "version": "0.16.29",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragestudio/nodecore"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron-webpack dev",
    "dev": "concurrently \"npm run dev:renderer\" \"electron src/main/index.js\"",
    "dev:renderer": "APP_ROOT=src/renderer BROWSER=none ESLINT=none umi dev",
    "build": "npm run build:renderer && npm run build:main",
    "build:main": "electron-webpack",
    "build:renderer": "APP_ROOT=src/renderer ESLINT=none umi build",
    "pack": "electron-builder",
    "pack:dir": "npm run build && electron-builder --dir",
    "pack:dirOnly": "electron-builder --dir"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "@babel/runtime": "^7.6.3",
    "antd": "^4.14.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-is": "^3.0.0",
    "electron-log": "^4.3.2",
    "electron-webpack": "^2.8.2",
    "umi": "^3.4.6",
    "@umijs/preset-react": "^1.8.2",
    "webpack": "^5.28.0"
  },
  "dependencies": {
    "webpack-node-externals": "^2.5.2",
    "wait-on": "5.3.0",
    "os-usage": "^0.0.3",
    "source-map-support": "^0.5.13"
  },
  "electronWebpack": {
    "renderer": null
  }
}
