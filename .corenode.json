{
  "version": "0.28.1",
  "builder": {
    "transforms": [
      "jsx",
      "imports"
    ],
    "ignorePackages": [
      "ual"
    ],
    "skip": [
      "packages/utils/src/verbosity.js"
    ]
  },
  "publish": {
    "ignorePackages": [
      "transcompiler"
    ],
    "include": [
      "./dist"
    ]
  },
  "development": {
    "headPackage": "corenode",
    "originGit": "https://github.com/ragestudio/corenode",
    "docs": [
      "corenode",
      "utils"
    ],
    "dockerImageName": "srgooglo/corenode"
  }
}
