{
  "name": "corenode",
  "version": "0.28.2",
  "git": "https://github.com/ragestudio/corenode",
  "funding": [
    {
      "url": "https://www.paypal.com/paypalme/RageStudioES"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "corenode-node": "./bin/corenode-node",
    "corenode-cli": "./bin/cli/index.js",
    "corenode": "./bin/core/index.js"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "**/**"
  ],
  "scripts": {
    "linkAll": "npm run builder && npm run link:dist",
    "link:dist": "cd ./dist && yarn link && cd - && yarn link 'corenode'",
    "builder": "node ./scripts/build.js",
    "cli:local": "cross-env LOCAL_BIN=true node ./dist/bin/cli/index.js",
    "cli:prod": "cross-env LOCAL_BIN=false node ./dist/bin/cli/index.js",
    "cli:debug": "cross-env LOCAL_BIN=true DEBUG=true node --inspect-brk ./dist/bin/cli/index.js",
    "core:local": "cross-env LOCAL_BIN=true node ./dist/bin/core/index.js",
    "core:prod": "cross-env LOCAL_BIN=false node ./dist/bin/core/index.js",
    "core:debug": "cross-env LOCAL_BIN=true DEBUG=true node --inspect-brk ./dist/bin/core/index.js",
    "release:next": "npm run builder && node ./dist/bin/cli/index.js publish --preRelease --npm --fast --github --packages",
    "release:prod": "npm run builder && node ./dist/bin/cli/index.js publish --npm --github --fast --packages",
    "update:deps": "yarn upgrade-interactive --latest",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "clean": "rm -rf ./build ./dist ./dist-self-build ./dist-types ./example-runner/example-repos ./test262/test262-checkout",
    "generate": "node scripts/generate.js"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@corenode/git-lib": "0.28.2",
    "@corenode/helpers": "0.28.2",
    "@corenode/pkg-manager": "0.28.2",
    "@corenode/utils": "0.28.2",
    "7zip-bin": "^5.1.1",
    "node-7z": "^3.0.0",
    "glob": "^7.1.7",
    "fast-glob": "^3.2.7",
    "lines-and-columns": "^1.1.6",
    "mz": "^2.7.0",
    "pirates": "^4.0.1",
    "ts-interface-checker": "^1.0.0",
    "commander": "^8.2.0",
    "cross-blob": "^3.0.0",
    "cross-env": "^7.0.3",
    "dargs": "^8.1.0",
    "dotenv": "^10.0.0",
    "escape-goat": "^4.0.0",
    "execa": "^5.1.1",
    "extract-first-json": "^1.0.1",
    "read-package-json": "^4.1.1",
    "filesize": "^8.0.3",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "npm": "^7.24.0",
    "npm-package-arg": "^8.1.5",
    "npm-registry-fetch": "^11.0.0",
    "open": "^8.2.1",
    "ora": "^6.0.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "smart-circular": "^1.0.2",
    "stacktrace-js": "^2.0.2",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34",
    "winston": "^3.3.3",
    "yargs-parser": "^20.2.9",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-do-expressions": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.13.10",
    "@types/glob": "7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/mz": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "vinyl-fs": "^3.0.3",
    "through2": "^4.0.2",
    "chalk": "2.4.1",
    "codecov": "^3.8.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^7.1.1",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "sucrase": "^3.20.1",
    "test262-harness": "^6.5.0",
    "ts-interface-builder": "^0.2.1",
    "typescript": "^4.1.3",
    "@types/babel__core": "7.1.12",
    "@types/babel__traverse": "7.11.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.31",
    "@vercel/ncc": "^0.28.6",
    "nexe": "^4.0.0-beta.18",
    "node-gyp": "^8.0.0",
    "nodemon": "^2.0.7",
    "pkg": "^5.2.1"
  }
}
