{
  "name": "corenode",
  "version": "0.26.12",
  "git": "https://github.com/ragestudio/corenode",
  "funding": [
    {
      "url": "https://www.paypal.com/paypalme/RageStudioES"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "corenode": "./bin/index.js"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "internals",
    "dist",
    "bin",
    "types.d.ts"
  ],
  "scripts": {
    "debug": "cross-env LOCAL_BIN=true DEBUG=true node --inspect-brk ./bin/index.js",
    "local": "cross-env LOCAL_BIN=true node ./bin/index.js",
    "prod": "cross-env LOCAL_BIN=false node ./bin/index.js",
    "builder": "node ./internals/builder/cli.js",
    "build:docs": "corenode docs --innerFiles",
    "release:next": "npm run builder && node ./bin/index.js publish --preRelease --next --npm --fast --github",
    "release:prod": "npm run builder && node ./bin/index.js publish --npm --github --fast",
    "update:deps": "yarn upgrade-interactive --latest",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-do-expressions": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.13.10",
    "@corenode/pkg-manager": "0.26.12",
    "@corenode/git-lib": "0.26.12",
    "@corenode/helpers": "0.26.12",
    "@corenode/utils": "0.26.12",
    "7zip-bin": "^5.1.1",
    "cli-progress": "^3.9.0",
    "cross-blob": "^2.0.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dargs": "^8.0.0",
    "dotenv": "^10.0.0",
    "emscripten-sdk-npm": "^0.3.2",
    "escape-goat": "^3.0.0",
    "execa": "^5.1.1",
    "extract-first-json": "^1.0.1",
    "fast-glob": "^3.2.5",
    "filesize": "^6.3.0",
    "listr": "0.14.3",
    "lodash": "^4.17.21",
    "map-stream": "^0.0.7",
    "md5": "^2.3.0",
    "node-7z": "^2.1.2",
    "npm": "^7.19.0",
    "npm-package-arg": "^8.1.5",
    "npm-registry-fetch": "^11.0.0",
    "open": "^8.0.8",
    "ora": "^5.4.1",
    "read-package-json": "^3.0.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "slash2": "^2.0.0",
    "smart-circular": "^1.0.2",
    "stacktrace-js": "^2.0.2",
    "threads": "^1.6.3",
    "through2": "^4.0.2",
    "uuid": "^8.3.2",
    "vinyl-fs": "^3.0.3",
    "websocket": "^1.0.34",
    "which": "^2.0.2",
    "winston": "^3.3.3",
    "yargs": "^17.0.1",
    "yargs-parser": "^20.2.7"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.12",
    "@types/babel__traverse": "7.11.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.31",
    "@vercel/ncc": "^0.28.6",
    "nexe": "^4.0.0-beta.18",
    "node-gyp": "^8.0.0",
    "nodemon": "^2.0.7",
    "pkg": "^5.2.1"
  }
}
