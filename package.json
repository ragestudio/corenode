{
  "git": "https://github.com/ragestudio/nodecorejs",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "kayser-build",
    "debug": "node --inspect-brk packages/nodecore/bin/cli.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "link:nodecore": "cd packages/nodecore && yarn link && cd -",
    "release": "node ./scripts/release.js",
    "sync:tnpm": "node -e 'require(\"./scripts/syncTNPM\")()'",
    "benchmark": "node scripts/benchmarks.js",
    "startExample": "cd ./examples/normal && nodecore dev",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@nodecorejs/core": "0.4.3",
    "@nodecorejs/dot-runtime": "0.4.3",
    "@nodecorejs/libs": "0.4.3",
    "@testing-library/react": "^11.2.2",
    "@types/babel__core": "7.1.12",
    "@types/babel__traverse": "7.0.16",
    "@types/find-up": "^4.0.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.10",
    "benchmark": "^2.1.4",
    "escape-goat": "^3.0.0",
    "find-up": "^5.0.0",
    "kayser-build": "^0.1.3001",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.2",
    "new-github-release-url": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragestudio/nodecorejs.git"
  },
  "dependencies": {
    "args-parser": "^1.1.0"
  }
}
