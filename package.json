{
  "name": "corenode",
  "version": "0.25.26",
  "git": "https://github.com/ragestudio/corenode",
  "funding": [
		{
			"url": "https://www.paypal.com/paypalme/RageStudioES"
		}
	],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "corenode": "./bin/index.js"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "internals/*/**/dist",
    "internals/objects/**/**",
    "dist",
    "types.d.ts"
  ],
  "workspaces": [
    "packages/**/*",
    "addons/**/*"
  ],
  "scripts": {
    "postinstall": "npm run initBuilder",
    "initBuilder": "node ./postinstall/index.js",
    "firstInstall": "cross-env INSTALL_INIT=true node ./postinstall/index.js",
    "debug": "cross-env LOCAL_BIN=true DEBUG=true node --inspect-brk ./packages/corenode/bin/index.js",
    "local": "cross-env LOCAL_BIN=true node ./packages/corenode/bin/index.js",
    "prod": "cross-env LOCAL_BIN=false node ./packages/corenode/bin/index.js",
    "builder": "node ./packages/builder/cli.js",
    "build:docs": "corenode docs --innerFiles",
    "release:next": "npm run builder && node ./packages/corenode/bin/index.js publish --preRelease --next --npm --fast --github",
    "release:prod": "npm run builder && node ./packages/corenode/bin/index.js publish --npm --github --fast",
    "link:main": "cd packages/corenode && yarn link && cd -",
    "update:deps": "yarn upgrade-interactive --latest",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "npm-package-arg": "^8.1.5",
    "npm-registry-fetch": "^11.0.0",
    "@npmcli/config": "^2.2.0",
    "read-package-json": "^3.0.1",
    "pacote": "^11.3.4",
    "libnpmpublish": "^4.0.2",
    "libnpmpack": "^2.0.1",
    "npm": "^7.19.0",
    "cross-spawn": "^7.0.3",
    "extract-first-json": "^1.0.1",
    "semver": "^7.3.5",
    "cross-blob": "^2.0.0",
    "fast-glob": "^3.2.5",
    "dargs": "^8.0.0",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2",
    "ora": "^5.4.1",
    "websocket": "^1.0.34",
    "7zip-bin": "^5.1.1",
    "node-7z": "^2.1.2",
    "listr": "0.14.3",
    "execa": "^5.1.1",
    "escape-goat": "^3.0.0",
    "yargs": "^17.0.1",
    "yargs-parser": "^20.2.7",
    "open": "^8.0.8",
    "stacktrace-js": "^2.0.2",
    "winston": "^3.3.3",
    "filesize": "^6.3.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "@babel/runtime": "^7.13.10",
    "@corenode/git-lib": "0.25.26",
    "@corenode/builder": "0.25.26",
    "@corenode/utils": "0.25.26",
    "smart-circular": "^1.0.2",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-do-expressions": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "7.1.12",
    "@types/babel__traverse": "7.11.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.31",
    "nodemon": "^2.0.7",
    "@vercel/ncc": "^0.28.6",
    "nexe": "^4.0.0-beta.18",
    "node-gyp": "^8.0.0",
    "pkg": "^5.2.1"
  }
}
