{
  "git": "https://github.com/srgooglo/nodecorejs",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "father-build",
    "debug": "node --inspect-brk packages/umi/bin/umi.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "link:umi": "cd packages/umi && yarn link && cd -",
    "release": "node ./scripts/release.js",
    "sync:tnpm": "node -e 'require(\"./scripts/syncTNPM\")()'",
    "benchmark": "node scripts/benchmarks.js",
    "startExample": "cd ./examples/normal && umi dev",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@nodecorejs/core": "0.2.0-alpha.0",
    "@nodecorejs/utils": "0.2.0-alpha.0",
    "@testing-library/react": "^10.4.6",
    "@types/babel__core": "7.1.9",
    "@types/babel__traverse": "7.0.13",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "benchmark": "^2.1.4",
    "escape-goat": "^3.0.0",
    "father-build": "^1.18.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "new-github-release-url": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.4",
    "prettier": "^2.0.5",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srgooglo/nodecorejs.git"
  }
}
